{
  "manifest_version": 2,

  "name": "Search Easy",
  "description": "A standard web search box to help you search content on websites",
  "version": "1.0",
  "background": {"scripts": ["jquery-2.1.4.min.js", "rules.js", "background.js"]                },
  "browser_action": {
    "default_icon": "searcheasy.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "commands":
	{
		"search_box" :
		{
			"suggested_key": {
				"default": "Alt+F",
				"mac": "Alt+F "
			},
			"description" : "command to show search box"
		}
	}
}
